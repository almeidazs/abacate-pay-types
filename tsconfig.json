{
	"compilerOptions": {
		"declaration": true,
		"emitDeclarationOnly": true,
		"outDir": "./dist",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"target": "ESNext",
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"paths": {
			"@/*": ["./types/*"]
		}
	},
	"include": ["types"]
}
